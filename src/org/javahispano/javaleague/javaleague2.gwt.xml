<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='javaleague2'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->
	<inherits name="org.gwtbootstrap3.GwtBootstrap3" />
	<inherits name="org.gwtbootstrap3.extras.respond.Respond" />
	<inherits name="org.gwtbootstrap3.extras.datetimepicker.DateTimePicker" />
	<inherits name="org.gwtbootstrap3.extras.summernote.Summernote" />
	<inherits name="org.gwtbootstrap3.extras.bootbox.Bootbox" />

	<inherits name="com.googlecode.objectify.Objectify" />
	<inherits name='com.google.web.bindery.requestfactory.RequestFactory' />

	<!-- Internacionalization -->
	<inherits name='com.google.gwt.i18n.I18N' />
	<!-- Spanish language, independent of country -->
	<extend-property name="locale" values="es" />
	<!-- English language, independent of country -->
	<extend-property name="locale" values="en" />
	<set-property-fallback name="locale" value="es" />
	<set-configuration-property name="locale.cookie"
		value="GWT_LOCALE" />

	<!-- Use ClientFactoryImpl by default -->
	<replace-with class="org.javahispano.javaleague.client.ClientFactoryImpl">
		<when-type-is class="org.javahispano.javaleague.client.ClientFactory" />
	</replace-with>

	<!-- Specify the app entry point class. -->
	<entry-point class='org.javahispano.javaleague.client.JavaLeague' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>